<!-- Navit can write a tracklog in several formats (gpx, nmea or textfile): -->
<!-- <log type="gpx" data="track_%Y%m%d-%i.gpx" flush_size="1000" flush_time="30"/> -->
<cursor w="48" h="48">
    <itemgra>
        <circle color="#ffffff" radius="20" width="20">
            <coord x="0" y="0"/>
        </circle>
        <circle color="#0000ff" radius="30" width="5">
            <coord x="0" y="0"/>
        </circle>
    </itemgra>
    <itemgra speed_range="-2">
        <polyline color="#0000ff" width="8">
            <coord x="0" y="0"/>
            <coord x="0" y="0"/>
        </polyline>
    </itemgra>
    <itemgra speed_range="3-">
        <polyline color="#0000ff" width="8">
            <coord x="0" y="0"/>
            <coord x="0" y="20"/>
        </polyline>
    </itemgra>
</cursor>
